package Helper;

public class IgnoreHelper {
	private static String[] listOfIgnore;
	public void init(String[] list){
		listOfIgnore = list;
	}
	public static boolean ifIgnore(String str){
		str.toLowerCase()
		for (String element: listOfIgnore){
			if (element.equals(str)){
				return true;
			}
		}
		return false;
	}
}
